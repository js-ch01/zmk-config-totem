#define NUM  2
#define SYS  3
#define ETC1 4
#define ETC2 5

// mod-tap

&mt {
    quick-tap-ms = <100>;
    global-quick-tap;
    flavor = "tap-preferred";
    tapping-term-ms = <300>;
};

// mouse key

#define U_MOUSE_MOVE_MAX 1250   /* default: 600 */
#define U_MOUSE_SCROLL_MAX 100  /* default: 10  */

&mmv {
    time-to-max-speed-ms = <400>;
};

&mwh {
    time-to-max-speed-ms = <500>;
};

